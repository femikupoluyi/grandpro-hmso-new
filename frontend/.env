# GrandPro HMSO Frontend Environment Variables

# API Configuration
VITE_API_URL=/api
VITE_PUBLIC_API_URL=/api

# Application Settings
VITE_APP_NAME=GrandPro HMSO
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Nigerian Localization
VITE_TIMEZONE=Africa/Lagos
VITE_CURRENCY=NGN
VITE_COUNTRY=Nigeria
VITE_DEFAULT_LANGUAGE=en-NG

# Feature Flags
VITE_ENABLE_TELEMEDICINE=true
VITE_ENABLE_AI_TRIAGE=true
VITE_ENABLE_LOYALTY_PROGRAM=true
VITE_ENABLE_NHIS_INTEGRATION=true

# Security
VITE_SESSION_TIMEOUT=3600000
VITE_MAX_LOGIN_ATTEMPTS=5

# Analytics
VITE_ENABLE_ANALYTICS=false
VITE_ANALYTICS_ID=

# Support
VITE_SUPPORT_EMAIL=support@grandpro.ng
VITE_SUPPORT_PHONE=+234 800 GRANDPRO
